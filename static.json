{
  "version": 1,
  "redirects": [
    {
      "from": "/index.html",
      "to": "/",
      "statusCode": 301
    },
    {
      "from": "/login.html",
      "to": "/login",
      "statusCode": 301
    },
    {
      "from": "/tietosuojas.html",
      "to": "/tietosuojas",
      "statusCode": 301
    },
    {
      "from": "/*",
      "to": "/index.html",
      "statusCode": 200
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/login",
      "dest": "/login.html"
    },
    {
      "src": "/tietosuojas",
      "dest": "/tietosuojas.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {"key": "Cache-Control", "value": "public, max-age=31536000"}
      ]
    }
  ]
}